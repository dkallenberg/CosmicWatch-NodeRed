    {
        "id": "f30598f4735b1b4a",
        "type": "function",
        "z": "6caafa316e63ba15",
        "name": "Min Rate",
        "func": "var data = msg.data;\nvar lastCount = global.get(\"minLastCount\");\nvar lastTime = global.get(\"minLastTime\");\nvar lastup = global.get(\"minLastUp\");\nvar uptime = msg.uptime;\nvar dTime = (uptime - lastup);\nif(dTime >=60){\n    global.set(\"minLastCount\", data[0]);\n    global.set(\"minLastTime\", data[1]);\n    global.set(\"minLastUp\", uptime);\n    var avg = (data[0]-lastCount) / dTime;\n    msg.payload = avg.toFixed(3);\n    msg.topic = \"1 Min\";\n    data[8]=avg.toFixed(3);\n    msg.data =data;\n    var msg2 ={payload: data[5], topic: \"Temp [C]\"};\n    return [msg, msg2, msg];\n} else{\n    return [ null, null, msg];\n}",
